<Window x:Class="ShFLU.SMGS.SMGS_View"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ShFLU.SMGS"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="SMGS_View"
        Width="520.725"
        Height="300"
        mc:Ignorable="d">
    <Grid>
        <Button x:Name="button"
                Width="93"
                Margin="10,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Command="{Binding LoadSmgsCommand}"
                Content="LOAD SMGS" />
        <Button x:Name="Serrial"
                Width="118"
                Margin="10,35,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Command="{Binding SerializeSmgsCommand}"
                Content="SerializeSmgs" />

    </Grid>
</Window>
